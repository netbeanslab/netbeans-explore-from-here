<filesystem>

    <folder name="Projects">
        <folder name="Actions">
            <file name="net.sf.efhnbm.ExploreFromHere.instance"/>
        </folder>
    </folder>
    <folder name="Loaders">
        <folder name="folder">
            <folder name="any">
                <folder name="Actions">
                    <file name="net.sf.efhnbm.ExploreFromHere.instance"/>
                </folder>
            </folder>
        </folder>    
    </folder>
 
    <folder name="Services">
        <file name="net-sf-efhnbm-options-classic-EFHSettings.settings" url="EFHSettings.xml"/>
        <folder name="AutoupdateType">
         <file name="efhnbm_autoupdate_center.settings" url="EFHAutoUpdate.xml">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="net.sf.efhnbm.Bundle"/>
            <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/updatecenters/resources/updateAction.gif"/>
            <attr name="url_key" stringvalue="URL_EFHNBM"/>
            <attr name="enabled" boolvalue="true"/>
         </file>
      </folder>
    </folder>
    
    <folder name="UI">
        <folder name="Services">
            <folder name="IDEConfiguration">
                <folder name="ServerAndExternalToolSettings">
                    <file name="net-sf-efhnbm-options-classic-EFHSettings.shadow">
                        <attr name="originalFile" stringvalue="Services/net-sf-efhnbm-options-classic-EFHSettings.settings"/>
                    </file>
                </folder>
            </folder>
        </folder>
    </folder>
    
    <folder name="OptionsDialog">
      <folder name="Advanced">
        <file name="EFHOptionCategory.instance">
            <attr name="instanceClass" stringvalue="net.sf.efhnbm.options.spi.EFHOptionCategory"/>
        </file>
      </folder>
    </folder>
 
</filesystem>
